{
  "routes": [
    {
      "routes": [
        {
          "route": "/*",
          "allowedRoles": [
            "authenticated"
          ],
          "statusCode": 401
        }
      ],
      "responseOverrides": {
        "401": {
          "statusCode": 302,
          "redirect": "/.auth/login/aad"
        }
      }
    }
  ]
}